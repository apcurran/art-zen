{
  "name": "art-zen",
  "version": "1.0.0",
  "description": "Art Zen is a social website for artists.",
  "main": "server.js",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "test": "jest",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run --config video=false,screenshotOnRunFailure=false",
    "lint": "eslint ./",
    "format": "npx prettier --write .",
    "start": "node --env-file=.env server.js",
    "dev": "node --env-file=.env --watch server.js",
    "avatar-img-update": "node --env-file=.env ./db/avatar-img-update-script.js",
    "db:seeds": "node --env-file=.env ./db/seeds.js",
    "heroku-postbuild": "cd client && npm clean-install --production && npm run build"
  },
  "author": "Alex Curran",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^1.28.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg-promise": "^12.3.0"
  },
  "devDependencies": {
    "cypress": "^9.5.1",
    "eslint": "^8.9.0",
    "eslint-plugin-cypress": "^2.12.1",
    "faker": "^5.5.3",
    "morgan": "^1.10.1",
    "prettier": "3.7.4"
  }
}
