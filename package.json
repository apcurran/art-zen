{
    "name": "art-zen",
    "version": "1.0.0",
    "description": "Art Zen is a social website for artists.",
    "main": "server.js",
    "engines": {
        "node": "24.x"
    },
    "scripts": {
        "test": "jest",
        "cy:open": "npx cypress open",
        "cy:run": "npx cypress run",
        "lint": "npx eslint ./",
        "format": "npx prettier --write .",
        "start": "node --env-file=.env server.js",
        "dev": "node --env-file=.env --watch server.js",
        "avatar-img-update": "node --env-file=.env ./db/avatar-img-update-script.js",
        "db:seeds": "node --env-file=.env ./db/seeds.js",
        "heroku-postbuild": "cd client && npm clean-install --production && npm run build"
    },
    "author": "Alex Curran",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^6.0.0",
        "cloudinary": "^2.8.0",
        "express": "^5.2.1",
        "express-rate-limit": "^8.2.1",
        "helmet": "^8.1.0",
        "joi": "^18.0.2",
        "jsonwebtoken": "^9.0.3",
        "pg-promise": "^12.3.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@eslint/json": "^0.14.0",
        "@eslint/markdown": "^7.5.1",
        "@faker-js/faker": "^10.1.0",
        "cypress": "^15.8.1",
        "eslint": "^9.39.2",
        "eslint-plugin-cypress": "^5.2.0",
        "eslint-plugin-react": "^7.37.5",
        "globals": "^17.0.0",
        "morgan": "^1.10.1",
        "prettier": "3.7.4"
    }
}
